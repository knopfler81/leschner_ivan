<div class="col-md-3">
	<div class="row">
		<div class="col-xs-12">
			<%= link_to "#",  class: "link_to" do %>
				<%= image_tag guitar.url, class: "small_image" %>
			<% end %>	
		</div>
		<div class="col-xs-12" >
			<% if current_user %>
				<%= link_to finished_guitar_attachment_path(@finished_guitar, index), :method => :delete, data: { confirm: "Are you sure you want to delete this image?" } do %>
					<i class="fas fa-trash-alt trash"></i>
				<% end %>
			<% end %>
		</div>
	</div>
</div>

